{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "laravel/framework": "^10.8",
        "laravel/jetstream": "^3.1",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "monarobase/country-list": "^3.4",
        "owen-it/laravel-auditing": "^13.5",
        "sentry/sentry-laravel": "^4.6",
        "spatie/laravel-backup": "^8.1",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^0.20.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "rector/rector": "^0.16.0",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "build": [
            "php artisan migrate:fresh",
            "php artisan db:seed --class=UsersTableSeeder",
            "php artisan db:seed --class=DatabaseSeeder"
        ],
        "dev": [
            "php artisan db:seed --class=DemoUsersSeeder",
            "php artisan db:seed --class=MembersTableSeeder"
        ],
        "lint": [
            "./vendor/bin/pint --test",
            "./vendor/bin/phpcs",
            "./vendor/bin/rector --dry-run"
        ],
        "lint_ci": [
            "./vendor/bin/pint --test",
            "./vendor/bin/phpcs -n"
        ],
        "format": [
            "./vendor/bin/pint",
            "./vendor/bin/phpcbf",
            "./vendor/bin/rector"
        ],
        "format_ci": [
            "./vendor/bin/pint",
            "./vendor/bin/phpcbf"
        ],
        "test": [
            "./vendor/bin/pest"
        ],
        "test_single": [
            "./vendor/bin/pest --filter MemberSubmissionTest"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "violinist": {
          "default_branch": "main",
          "security_updates_only": 1,
          "update_dev_dependencies": 0,
          "update_with_dependencies": 1
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
